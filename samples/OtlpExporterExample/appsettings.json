{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "OpenTelemetry": {
    "Exporters": {
      "OTLP": {
        "Enabled": true,
        "Protocol": "HttpProtobuf",
        "Endpoint": "http://localhost:5341/ingest/otlp",
        "Signals": "TracesAndLogs"
      }
    }
  }
}
